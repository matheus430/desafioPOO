package funcoes.navegador;

public class AbreNavegador implements NavegadorInternet {

	public void navegar() {
		abrirNavegador();
		checarInternet();
		System.out.println("Navegando...");
	}
	
	private void abrirNavegador() {
		System.out.println("Abrir navegador!");
	}
	
	private void checarInternet() {
		System.out.println("Conferindo conectividade...");
	}

}
