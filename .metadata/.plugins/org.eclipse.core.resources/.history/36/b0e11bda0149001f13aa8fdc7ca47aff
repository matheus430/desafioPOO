		CloseBrowser safariClose = new CloseBrowser();
		safariClose.closeB();
	}
	private void checkConnection() {
		System.out.println("Cheking connection" + "\n");
	}
	
	public void saveHistoric() {
		System.out.println("Saving historic" + "\n");
	}
}
