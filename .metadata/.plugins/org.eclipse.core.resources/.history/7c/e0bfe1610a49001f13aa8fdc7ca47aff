package iphone.browser;

public class OpenBrowser implements Browser {
	public void openB() {
		System.out.println("Open browser" + "\n");
		checkConnection();
		Navigate();
	}
	private void checkConnection() {
		System.out.println("Cheking connection..." + "\n");
	}
	
	private void Navigate() {
		System.out.println("Navigation..." + "\n");
	}

}
