package iphone.browser;

public class Browser  {
	public static void main(String[] args) {
		OpenBrowser safariOpen = new OpenBrowser();
		safariOpen.openB();
		
		OpenBrowser checkingConnection = new OpenBrowser();
		checkingConnection.checkConnection();
		
		OpenBrowser navigating = new OpenBrowser();
		navigating.Navigate();
		
		CloseBrowser safariClose = new CloseBrowser();
		safariClose.closeB();
	}
	

}
